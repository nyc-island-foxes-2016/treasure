<div class="col-sm-6 col-md-4">
  <div class="thumbnail">
  	<%= image_tag match.other(@item).avatar.url, width: "100", height:"100" %>
    <div class="caption">
      <h3><%= link_to match.other(@item).name, item_match_path(@item, match) %></h3>
      <p><%= match.other(@item).description %></p>
    </div>
  </div>
  <div class="details">
        <i class="fa fa-comment-o"><%= link_to match.messages.count, item_match_path(@item, match) %></i>
  </div>
</div>
